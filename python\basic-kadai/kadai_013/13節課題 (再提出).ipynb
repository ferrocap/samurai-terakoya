        "def add_two_arguments(price, tax):\n",
        "    # 与えられた引数priceと引数taxを乗算し、変数totalに代入する\n",
        "    total = price * (1+tax/100)\n",
        "    return total\n",
        "\n",
        "# 関数を呼び出し、引数として購入金額と消費税を渡す\n",
        "# 変数$totalの値を出力する\n",
        "print(f\"{int(add_two_arguments(110, 10))}円\")"
      ]
